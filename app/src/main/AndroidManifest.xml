        <activity android:name=".Finance" />
        <activity android:name=".Engineering" />
        <activity android:name=".Educational" />
        <activity android:name=".Construction" />
        <activity android:name=".Architecture" />
        <activity android:name=".Featured" />
        <activity android:name=".Settings" />
        <activity android:name=".Profile" />
        <activity android:name=".Categories" />
        <activity android:name=".ResetPassword" />
        <activity android:name=".ProView" />
        <activity android:name=".ProSignup" />
        <activity android:name=".UserSignup" />
        <activity android:name=".Login" />
        <activity android:name=".Welcome" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>